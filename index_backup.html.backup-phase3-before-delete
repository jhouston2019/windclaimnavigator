    // close on link click
    document.querySelectorAll('.mobile-menu a').forEach(link => {
      link.addEventListener('click', () => {
        hamburger.classList.remove('active');
        mobileMenu.classList.remove('show');
      });
    });

    // close menu when clicking outside
    document.addEventListener('click', (e) => {
      if (!hamburger.contains(e.target) && !mobileMenu.contains(e.target)) {
        hamburger.classList.remove('active');
        mobileMenu.classList.remove('show');
      }
    });
    
    function toggleLanguage() {
      currentLang = currentLang === 'en' ? 'es' : 'en';
      
      // Update button text
      const button = document.getElementById('lang-toggle-btn');
      if (button) {
        button.textContent = currentLang === 'en' ? 'For Spanish' : 'For English';
        console.log('Language toggled to:', currentLang, 'Button text:', button.textContent);
      }
      
      // Update page elements
      const elements = document.querySelectorAll('[data-en][data-es]');
      elements.forEach(function(element) {
        const text = element.getAttribute('data-' + currentLang);
        if (text) {
          if (element.tagName === 'BUTTON' || element.tagName === 'INPUT') {
            element.textContent = text;
          } else {
            element.innerHTML = text;
          }
        }
      });
      
      // Update title
      document.title = currentLang === 'es' 
        ? 'Claim Navigator - Herramientas de Documentación con IA para Reclamos'
        : 'Claim Navigator - AI-Powered Claim Documentation Tools';
      
      // Save preference
      localStorage.setItem('preferredLanguage', currentLang);
    }
    
    
    // Initialize
    document.addEventListener('DOMContentLoaded', function() {
      const saved = localStorage.getItem('preferredLanguage');
      if (saved === 'es') {
        toggleLanguage();
      }
      // Button already starts with 'For Spanish' so no need to set it
    });
    
    // Testimonial Slider Auto-Rotation
    let slideIndex = 0;
    const slides = document.querySelectorAll('.testimonial-slider .slide');
    
    function showSlides() {
      slides.forEach(s => s.classList.remove('active'));
      slideIndex = (slideIndex + 1) % slides.length;
      if (slides[slideIndex]) {
        slides[slideIndex].classList.add('active');
      }
    }
    
    // Start slider after DOM is loaded
    document.addEventListener('DOMContentLoaded', function() {
      if (slides.length > 0) {
        setInterval(showSlides, 5000);
      }
    });
    
    // Stripe Checkout Integration
    async function startCheckout() {
      try {
        const res = await fetch("/.netlify/functions/checkout", {
          method: "POST",
          headers: { "Content-Type": "application/json" },
          body: JSON.stringify({ priceId: "price_1SFM1K7CdVcPUl7l8sQDsC6Y" }),
        });
        const data = await res.json();
        if (!res.ok) throw new Error(data.error || "Invalid request");
        window.location.href = data.url;
      } catch (err) {
        console.error("Checkout error:", err);
        alert(err.message);
      }
    }
    
    // FAQ Toggle
    function toggleFAQ(question) {
      const answer = question.nextElementSibling;
      const icon = question.querySelector('span');
      answer.classList.toggle('active');
      icon.textContent = answer.classList.contains('active') ? '▲' : '▼';
    }
    
    // Smooth scroll for anchor links
    document.querySelectorAll('a[href^="#"]').forEach(anchor => {
      anchor.addEventListener('click', function (e) {
        e.preventDefault();
        const target = document.querySelector(this.getAttribute('href'));
        if (target) {
          target.scrollIntoView({ behavior: 'smooth', block: 'start' });
        }
      });
    });
  </script>

  <!-- Affiliate Tools Script -->
  <script>
    // Inject affiliate ID from environment variable
    window.AFFILIATE_ID = "";
  </script>
  <script src="assets/js/affiliate-tools.js"></script>

<script type="module" src="/app/assets/js/diagnostics.js"></script>
<script type="module" src="/app/assets/js/window-bridge.js"></script>

<script>
// TESTIMONIAL SLIDER
const tSlides = document.querySelectorAll('.t-slide');
let ti = 0;
setInterval(() => {
  tSlides[ti].classList.remove('active');
  ti = (ti + 1) % tSlides.length;
  tSlides[ti].classList.add('active');
}, 5200);
</script>
</body>
</html>
